<script>
(function($) {
var processing = false,
    captchaDisplayed = false,
    showingPreLogin = true,
    loginEmailUpdated = false;

function validatePreLogin() {
    var emailInput = $('#pre-login-email'),
        loginEmailInput = $('#login-email'),
        email = $.trim(emailInput.val());

    hideAllErrors();

    if (email === '') {
        emailInput.focus();
        showError(showingPreLogin ? emailInput : loginEmailInput, GT.gettext('Input your email address'));
        return false;
    }

    return true;
}

function submitPreLogin() {
    if (processing || !validatePreLogin()) {
        return;
    }

    processing = true;

    $.post('/ajax/sso_state', $('#pre-login-form').serialize(), function(response) {
        processing = false;

        switch (response.state) {
            case 'invalid':
                formErrorMessage("Page expired, please reload");
                break;
            case 'required':
                $('.public-form').addClass('public-form--require-sso');
                $('#pre-login-form').removeClass('hidden');
                $('#login-form').addClass('hidden');
                window.location = response.sso_url;
                break;
            default:
                $('#login-email').val($('#pre-login-email').val());
                $('#pre-login-form').addClass('hidden');
                $('#login-form').removeClass('hidden');
                $('#login-password').focus();
                loginEmailUpdated = false;
                showingPreLogin = false;
        }
    });
}

function validateLogin() {
    var emailInput = $('#login-email'),
        email = $.trim(emailInput.val()),
        passwordInput = $('#login-password'),
        password = passwordInput.val();

    hideAllErrors();

    if (password === '') {
        passwordInput.focus();
        showError(passwordInput, GT.gettext('Input your password'));
        return false;
    }

    if (captchaDisplayed && (grecaptcha.getResponse() === '')) {
        showError($('#login_captcha_container'), GT.gettext('We cannot verify your Captcha input'));
        return false;
    }

    return true;
}

function formErrorMessage(message) {
    $('.login-form-message')
        .find('.alert-content')
            .text(message)
            .end()
        .removeClass('hidden');
}

function hideAllErrors() {
    $('.has-error').removeClass('has-error');
    $('.help-block').css('display', 'none');
    $('.login-form-message').addClass('hidden');
}

function showError(element, message) {
    element.closest('.form-group')
        .addClass('has-error')
        .find('.help-block')
        .empty()
        .text(message)
        .show();
}

function submitLogin() {
    if (loginEmailUpdated || processing || !validateLogin()) {
        return;
    }

    processing = true;

    var buttonText = $('#btn-login').text();
    $('#btn-login').text('Logging in...');
    $.post('/ajax/doLogin', $('#login-form').serialize(), function(response) {
        parseResponse(response);

        if (responseArray.code == 0) {
            amplitudeTrackEvent(
                AMPLITUDE_EVENT.LOGIN,
                {
                    method: AMPLITUDE_EVENT_PROPERTIES.LOGIN_METHOD_PASSWORD,
                    referral: document.referrer,
                }
            );
            $(document).trigger('user.login');
        } else if (responseArray.code == 7) {
            processing = false;
            $('#btn-login').text(buttonText);

            $('#multiple-account-overlay').modal();
            $('#force-login-btn').focus();
        } else {
            processing = false;
            formErrorMessage(responseArray.json.errmsg);
            $('#btn-login').text(buttonText);

            $('#multiple-account-overlay').modal('hide');

            // If return throttle, show captcha
            if (captchaDisplayed) {
                grecaptcha.reset();
            } else if (responseArray.code == 6) {
                showCaptchaWidget();
            }
        }
    });
}

$('#pre-login-form').submit(function() {
    submitPreLogin();
    return false;
});

$('#pre-login-email').blur(function() {
    if ($(this).val() !== '') {
        submitPreLogin();
    }

    return false;
});

$('#login-email').on('input', function() {
    loginEmailUpdated = true;

    $('#pre-login-email').val(this.value);
}).on('blur', function() {
    if (loginEmailUpdated) {
        submitPreLogin();
    }
});

$('#login-form').submit(function() {
    submitLogin();
    return false;
});

$('#force-login-btn').click(function() {
    $('#force-login-input').val(true);
    submitLogin();
    return false;
});

function showCaptchaWidget() {
    grecaptcha.render('login_captcha_container', {"sitekey":"6LcvJjkUAAAAAIlJ2EwBKlDHYcJCykm0xXeew0oG","theme":"light"});
    captchaDisplayed = true;
}

window.onRecaptchaLoaded = function() {
};

$('#pre-login-email').focus();



})(jQuery);
</script>
<script src="http://web.archive.org/web/20180614055515js_/https://www.google.com/recaptcha/api.js?onload=onRecaptchaLoaded&amp;render=explicit" async defer></script>


